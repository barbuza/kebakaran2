{
  "name": "kebakaran",
  "version": "2.0.0",
  "description": "strongly typed high level utilities for consuming data from firebase",
  "main": "dist/lib/index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "build": "rimraf lib && tslint -p tsconfig.json && tsc && jest && rimraf lib/__tests__"
  },
  "author": {
    "name": "Victor Kotseruba",
    "url": "https://github.com/barbuza"
  },
  "license": "ISC",
  "peerDependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.12",
    "firebase": "^3.7.5",
    "immutable": "^3.8.1",
    "jest": "^19.0.2",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^19.0.8",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "@types/firebase": "^2.4.31",
    "es6-promise": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "roots": [
      "<rootDir>"
    ],
    "testRegex": "/__tests__/[^/]+-test\\.(ts|js)$",
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
